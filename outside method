//to declare method outside the class
#include <iostream>
using namespace std;

class Student {
public:
    string name;
    float age;
    string branch;
    float result;

    void Year() const; 
};

void Student::Year() const {
    cout << "I'm studying in second year" << endl;
}

int main() {
    Student s1;
    s1.name = "Ananya";
    s1.age = 18;
    s1.branch = "ENTC";
    s1.result = 89.98;

    cout << "Name of student 1 is: " << s1.name << endl;
    cout << "Age of student 1 is: " << s1.age << endl;
    cout << "Branch of student 1 is: " << s1.branch << endl;
    cout << "Result of student 1 is: " << s1.result << endl;
    s1.Year(); 

    return 0;
}
/* output:

Name of student 1 is: Ananya
Age of student: 18
Branch of student is: ENTC
Result of student: 89.98
I'm studying in second year

*/